<div class="row">
  <div class="col-md-8">
    <h1>All Wikis</h1>
    <% @wikis.each do |wiki| %>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
            <%= link_to wiki.title, wiki %>
          </h4>
        </div>
      </div>
    <% end %>
  </div>
  <div class="col-md-4">
    <%= link_to "New Wiki", new_wiki_path(@wiki), class: 'btn btn-success', style: 'margin-top: 25px; margin-left: 20px;' %>
  </div>
</div>
